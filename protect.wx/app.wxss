/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content:space-around;
  box-sizing: border-box;
  background-color: #f5f5f5;
} 

.pull-right{
	float:right;
}
.pull-left{
	float:left;
}
.clearfix:after{
	content: '';
	display:block;
	height:0;
	visibility: hidden;
	clear:both;
}